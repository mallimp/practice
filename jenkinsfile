pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Checkout code from Git repository
                git 'https://github.com/mallimp/practice.git'
            }
        }

        stage('Build') {
            steps {
                // Execute build commands here
                // For example, if you're building a Maven project
                sh 'mvn clean package'
            }
        }
    }
}
